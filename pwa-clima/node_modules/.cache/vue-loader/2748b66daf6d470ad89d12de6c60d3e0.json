ma', //NOMBRE DE CLIMA-- STORE-- AJAX\r\n    data(){\r\n        return{\r\n            estado:true,\r\n            estado2:false\r\n        }\r\n    },\r\n\r\n  computed:{\r\n    ...mapState(['clima']),\r\n  },\r\n  methods: {\r\n     cambioEstado:function()\r\n     {\r\n        this.estado = false\r\n        this.estado2 = true\r\n         \r\n         \r\n     },\r\n     cambioEstado2:function()\r\n     {\r\n  \r\n            this.estado = true\r\n            this.estado2 = false\r\n         \r\n     }\r\n  },\r\n}\r\n\r\n</script>"]}]}